{
  "name": "xtate",
  "version": "2.0.2",
  "description": "Simple State Managment library for React",
  "main": "lib/xtate.min.js",
  "types": "lib/xtate.d.ts",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielBB/xtate.git"
  },
  "keywords": [
    "xtate",
    "state-managment",
    "react",
    "redux"
  ],
  "author": "Gabriel Basilio Brito",
  "bugs": {
    "url": "https://github.com/GabrielBB/xtate/issues"
  },
  "homepage": "https://github.com/GabrielBB/xtate",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "react": "^16.2.0",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "fbemitter": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  }
}